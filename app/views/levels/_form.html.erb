<div class="left">
<%= form_for([@level.program,@level],html: { multipart: true, class: "form-horizontal" }) do |f| %>
  <% if @level.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(@level.errors.count, "error") %> prohibited this level from being saved:</h2>

      <ul>
      <% @level.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="form-group">
   <div class="col-md-3">
    <%= f.label :name %><br>
    <%= f.text_field :name,placeholder: 'level name',class: 'form-control'  %>
  </div>
  </div>
   <div class="form-group">
   <div class="col-md-3">
    <%= f.label :duration %><br>
    <%= f.text_field :duration,placeholder: 'level duration',class: 'form-control'  %>
  </div>
  </div>
   <div class="form-group">
    <%= f.submit class: 'btn btn-primary' %>
  </div>
<% end %>
</div>