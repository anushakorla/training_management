%p#notice= notice
%p
  %strong Title:
  = @test.title
%p
  %strong Questions:
  = @test.description
%p
  %strong Duration(days):
  = @test.duration
%p
  %strong Due Date:
  = @test.date
- if admin_signed_in?
  = link_to 'Edit', edit_level_test_path(@test.level,@test),class: 'btn btn-success'
  |
  =link_to 'Back', level_tests_path(@test.level),class: 'btn btn-success'
- if user_signed_in?
  = link_to 'Submit', new_test_submit_path(@test), class: 'btn btn-success'